<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    
    <bean class="springprogram5.Address" id="addrID">
    	<constructor-arg value="1" />
    	<constructor-arg value="Begusarai" />
    	<constructor-arg value="851101" />
    </bean>
    
    <bean class="springprogram5.Student" id="stdID">
    	<constructor-arg value="Ravi" />
    	<constructor-arg value="44" />
    	<constructor-arg ref="addrID" />
    </bean>

</beans>